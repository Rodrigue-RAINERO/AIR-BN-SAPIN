<h1>New tree</h1>
<%= form_with model: @tree do |t| %>
  <%= t.label :titre %>
  <%= t.text_field :titre %>
  <%= t.label :taille %>
  <%= t.text_field :taille %>
  <%= t.label :price %>
  <%= t.text_field :price %>
  <%= t.label :geoloc %>
  <%= t.text_field :geoloc %>
  <%= t.label :description %>
  <%= t.text_field :description %>
  <%= t.submit %>
<% end %>
